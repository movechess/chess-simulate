  "types": "./node.d.ts",
  "exports": {
    "./cjs/package.json": "./cjs/package.json",
    "./cjs/*": "./cjs/*.js",
    ".": {
      "types": "./node.d.ts",
      "react-native": {
        "module": "./react-native.js",
        "require": "./cjs/react-native.js",
        "default": "./react-native.js"
      },
      "browser": {
        "module": "./browser.js",
        "require": "./cjs/browser.js",
        "default": "./browser.js"
      },
      "node": {
        "module": "./node.js",
        "require": "./cjs/node.js",
        "default": "./node.js"
      }
    },
    "./browser": {
      "types": "./browser.d.ts",
      "module": "./browser.js",
      "require": "./cjs/browser.js",
      "default": "./browser.js"
    },
    "./detectOther": {
      "types": "./detectOther.d.ts",
      "module": "./detectOther.js",
      "require": "./cjs/detectOther.js",
      "default": "./detectOther.js"
    },
    "./detectPackage": {
      "types": "./detectPackage.d.ts",
      "module": "./detectPackage.js",
      "require": "./cjs/detectPackage.js",
      "default": "./detectPackage.js"
    },
    "./empty": {
      "types": "./empty.d.ts",
      "module": "./empty.js",
      "require": "./cjs/empty.js",
      "default": "./empty.js"
    },
    "./node": {
      "types": "./node.d.ts",
      "module": "./node.js",
      "require": "./cjs/node.js",
      "default": "./node.js"
    },
    "./package.json": {
      "require": "./cjs/package.json",
      "default": "./package.json"
    },
    "./packageInfo.js": {
      "types": "./packageInfo.d.ts",
      "module": "./packageInfo.js",
      "require": "./cjs/packageInfo.js",
      "default": "./packageInfo.js"
    },
    "./packageInfo": {
      "types": "./packageInfo.d.ts",
      "module": "./packageInfo.js",
      "require": "./cjs/packageInfo.js",
      "default": "./packageInfo.js"
    },
    "./react-native": {
      "types": "./react-native.d.ts",
      "module": "./react-native.js",
      "require": "./cjs/react-native.js",
      "default": "./react-native.js"
    },
    "./types": {
      "types": "./types.d.ts",
      "module": "./types.js",
      "require": "./cjs/types.js",
      "default": "./types.js"
    },
    "./util": {
      "types": "./util.d.ts",
      "module": "./util.js",
      "require": "./cjs/util.js",
      "default": "./util.js"
    }
  },
  "dependencies": {
    "@ledgerhq/hw-transport": "^6.28.8",
    "@ledgerhq/hw-transport-webhid": "^6.27.19",
    "@ledgerhq/hw-transport-webusb": "^6.27.19",
    "@polkadot/util": "12.5.1",
    "tslib": "^2.6.2"
  },
  "optionalDependencies": {
    "@ledgerhq/hw-transport-node-hid-singleton": "^6.28.18"
  }
